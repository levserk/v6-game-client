<% _.each(rooms, function(room) { %>
<tr class="userListGame" data-id="<%= room.room %>">
    <td class="userName" title="<%= room.players[0].userName + ' (' +  room.players[0].getRank(room.mode) + ')' %>" ><%= room.players[0].userName %></td>
    <td>:</td>
    <td class="userName" title="<%= room.players[1].userName + ' (' +  room.players[1].getRank(room.mode) + ')' %>" ><%= room.players[1].userName %></td>
</tr>
<% }) %>